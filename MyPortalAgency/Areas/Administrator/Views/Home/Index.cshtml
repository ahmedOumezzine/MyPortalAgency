    <!-- Specific Page Vendor -->
    <script src="~/portal\owl.carousel\owl.carousel.js"></script>
    <script>
       $('.owl-carousel').owlCarousel({
            loop: true,
            margin: 10,
            nav: true,
            navText: ["", ""],
            responsive: {
